@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div id="cabecero">

</div>
<div id="content" onload="perroflautas()">

</div>
<div id="formulario" >
    <form action="post">
        Nombre<input type="text" id="txtNombre" />
        Id<input type="text" id="txtId" />
        Orientacion<input type="text" id="txtOrientacion" />
        Direccion<input type="text" id="txtDireccion" />
        Posesiones<input type="text" id="txtPosesiones" />
        Grito-Comun<input type="text" id="txtGritComun" />
        Grito-Apareamiento<input type="text" id="txtGritApa" />
        <button>Agregar</button>
    </form>
</div>
<script>
    function perroflautas() {
            var tabla = '<table id="table">';
            tabla += "<th></th>";

            tabla += "@foreach (var er in Model.perroflautas) { <tr><td>@er.Id</td><td>@er.Nombre</td><td>@er.Direccion</td><td>@er.OrientacionIdeologica</td><td>@er.Posesiones</td><td>@er.GritoComun</td><td>@er.GritoAparea</td></tr> } ";

            tabla += "</table>";
                content.innerHTML = tabla;
            }
</script>
public int Id { get; set; }
        public string Nombre { get; set; }
        public string OrientacionIdeologica { get; set; }
        public string Direccion { get; set; }
        public string Posesiones { get; set; }
        public string GritoComun { get; set; }
        public string GritoAparea { get; set; }